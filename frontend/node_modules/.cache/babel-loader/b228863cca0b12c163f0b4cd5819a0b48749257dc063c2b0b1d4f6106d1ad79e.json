{"ast":null,"code":"import { ref, computed, onMounted, watch } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport AppLoader from '@/components/AppLoader.vue';\nimport AppNotifications from '@/components/AppNotifications.vue';\nimport { useNotificationStore } from '@/stores/notifications';\nimport api from '@/plugins/axios';\nexport default {\n  __name: 'ResetPassword',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const router = useRouter();\n    const notificationStore = useNotificationStore();\n\n    // Reactive data\n    const loading = ref(false);\n    const tokenVerified = ref(false);\n    const tokenError = ref('');\n    const passwordResetSuccess = ref(false);\n    const partnerInfo = ref(null);\n    const isFirstTimeSetup = ref(true); // –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é —Å—á–∏—Ç–∞–µ–º —á—Ç–æ —ç—Ç–æ –ø–µ—Ä–≤–∞—è —É—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø–∞—Ä–æ–ª—è\n\n    const formData = ref({\n      password: '',\n      confirmPassword: ''\n    });\n    const errors = ref({});\n    const showPassword = ref(false);\n    const showConfirmPassword = ref(false);\n\n    // Computed properties\n    const passwordStrength = computed(() => {\n      const password = formData.value.password;\n      if (!password) return {\n        width: '0%',\n        class: '',\n        text: ''\n      };\n      let score = 0;\n      let feedback = [];\n\n      // Length check\n      if (password.length >= 6) score += 1;\n      if (password.length >= 8) score += 1;\n      if (password.length >= 12) score += 1;\n\n      // Character variety checks\n      if (/[a-z]/.test(password)) score += 1;\n      if (/[A-Z]/.test(password)) score += 1;\n      if (/[0-9]/.test(password)) score += 1;\n      if (/[^A-Za-z0-9]/.test(password)) score += 1;\n\n      // Determine strength\n      let strength = '';\n      let width = '0%';\n      let className = '';\n      if (score <= 2) {\n        strength = '–°–ª–∞–±—ã–π';\n        width = '25%';\n        className = 'weak';\n      } else if (score <= 4) {\n        strength = '–°—Ä–µ–¥–Ω–∏–π';\n        width = '50%';\n        className = 'medium';\n      } else if (score <= 6) {\n        strength = '–•–æ—Ä–æ—à–∏–π';\n        width = '75%';\n        className = 'good';\n      } else {\n        strength = '–û—Ç–ª–∏—á–Ω—ã–π';\n        width = '100%';\n        className = 'excellent';\n      }\n      return {\n        width,\n        class: className,\n        text: strength\n      };\n    });\n    const isFormValid = computed(() => {\n      return formData.value.password.length >= 6 && formData.value.password === formData.value.confirmPassword && !Object.keys(errors.value).length;\n    });\n\n    // Methods\n    const verifyToken = async () => {\n      const token = route.query.token;\n      if (!token) {\n        tokenError.value = '–¢–æ–∫–µ–Ω –Ω–µ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω';\n        return;\n      }\n      try {\n        loading.value = true;\n        const response = await api.get(`/auth/reset-password/${token}`);\n        partnerInfo.value = {\n          partnerId: response.data.partnerId,\n          email: response.data.partnerEmail\n        };\n        tokenVerified.value = true;\n      } catch (error) {\n        console.error('–û—à–∏–±–∫–∞ –ø—Ä–æ–≤–µ—Ä–∫–∏ —Ç–æ–∫–µ–Ω–∞:', error);\n        tokenError.value = error.response?.data?.message || '–ù–µ–¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–π –∏–ª–∏ –∏—Å—Ç–µ–∫—à–∏–π —Ç–æ–∫–µ–Ω';\n      } finally {\n        loading.value = false;\n      }\n    };\n    const validateForm = () => {\n      errors.value = {};\n\n      // Password validation\n      if (!formData.value.password) {\n        errors.value.password = '–ü–∞—Ä–æ–ª—å –æ–±—è–∑–∞—Ç–µ–ª–µ–Ω';\n      } else if (formData.value.password.length < 6) {\n        errors.value.password = '–ü–∞—Ä–æ–ª—å –¥–æ–ª–∂–µ–Ω —Å–æ–¥–µ—Ä–∂–∞—Ç—å –º–∏–Ω–∏–º—É–º 6 —Å–∏–º–≤–æ–ª–æ–≤';\n      }\n\n      // Confirm password validation\n      if (!formData.value.confirmPassword) {\n        errors.value.confirmPassword = '–ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ –ø–∞—Ä–æ–ª—è –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ';\n      } else if (formData.value.password !== formData.value.confirmPassword) {\n        errors.value.confirmPassword = '–ü–∞—Ä–æ–ª–∏ –Ω–µ —Å–æ–≤–ø–∞–¥–∞—é—Ç';\n      }\n      return Object.keys(errors.value).length === 0;\n    };\n    const resetPassword = async () => {\n      if (!validateForm()) return;\n      const token = route.query.token;\n      loading.value = true;\n      try {\n        await api.post('/auth/reset-password', {\n          token,\n          password: formData.value.password\n        });\n        passwordResetSuccess.value = true;\n        notificationStore.addNotification({\n          type: 'success',\n          message: '–ü–∞—Ä–æ–ª—å —É—Å–ø–µ—à–Ω–æ –æ–±–Ω–æ–≤–ª–µ–Ω!'\n        });\n      } catch (error) {\n        console.error('–û—à–∏–±–∫–∞ —Å–±—Ä–æ—Å–∞ –ø–∞—Ä–æ–ª—è:', error);\n        notificationStore.addNotification({\n          type: 'error',\n          message: error.response?.data?.message || '–û—à–∏–±–∫–∞ —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –ø–∞—Ä–æ–ª—è'\n        });\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Watchers\n    watch([() => formData.value.password, () => formData.value.confirmPassword], () => {\n      if (errors.value.password || errors.value.confirmPassword) {\n        validateForm();\n      }\n    });\n\n    // Lifecycle\n    onMounted(() => {\n      verifyToken();\n    });\n    const __returned__ = {\n      route,\n      router,\n      notificationStore,\n      loading,\n      tokenVerified,\n      tokenError,\n      passwordResetSuccess,\n      partnerInfo,\n      isFirstTimeSetup,\n      formData,\n      errors,\n      showPassword,\n      showConfirmPassword,\n      passwordStrength,\n      isFormValid,\n      verifyToken,\n      validateForm,\n      resetPassword,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      get useRoute() {\n        return useRoute;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      AppLoader,\n      AppNotifications,\n      get useNotificationStore() {\n        return useNotificationStore;\n      },\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","useRoute","useRouter","AppLoader","AppNotifications","useNotificationStore","api","route","router","notificationStore","loading","tokenVerified","tokenError","passwordResetSuccess","partnerInfo","isFirstTimeSetup","formData","password","confirmPassword","errors","showPassword","showConfirmPassword","passwordStrength","value","width","class","text","score","feedback","length","test","strength","className","isFormValid","Object","keys","verifyToken","token","query","response","get","partnerId","data","email","partnerEmail","error","console","message","validateForm","resetPassword","post","addNotification","type"],"sources":["/Users/vadim/Desktop/freedom-group/frontend/src/views/auth/ResetPassword.vue"],"sourcesContent":["<template>\n  <div class=\"reset-password-page\">\n    <div class=\"container\">\n      <div class=\"reset-card\">\n        <div class=\"logo\">\n          <h1>FREEDOM GROUP</h1>\n          <p>{{ isFirstTimeSetup ? '–£—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø–∞—Ä–æ–ª—è' : '–í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–∞—Ä–æ–ª—è' }}</p>\n        </div>\n        \n        <!-- Loading state -->\n        <div v-if=\"loading && !tokenVerified && !tokenError\" class=\"loading-state\">\n          <AppLoader />\n          <p>–ü—Ä–æ–≤–µ—Ä–∫–∞ —Ç–æ–∫–µ–Ω–∞...</p>\n        </div>\n        \n        <!-- Error state -->\n        <div v-else-if=\"tokenError\" class=\"error-state\">\n          <div class=\"error-icon\">‚ö†Ô∏è</div>\n          <h3>–û—à–∏–±–∫–∞</h3>\n          <p>{{ tokenError }}</p>\n          <router-link to=\"/\" class=\"btn btn-primary\">–í–µ—Ä–Ω—É—Ç—å—Å—è –∫ –≤—Ö–æ–¥—É</router-link>\n        </div>\n        \n        <!-- Success state -->\n        <div v-else-if=\"passwordResetSuccess\" class=\"success-state\">\n          <div class=\"success-icon\">‚úÖ</div>\n          <h3>–ü–∞—Ä–æ–ª—å —É—Å–ø–µ—à–Ω–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω!</h3>\n          <p>–¢–µ–ø–µ—Ä—å –≤—ã –º–æ–∂–µ—Ç–µ –≤–æ–π—Ç–∏ –≤ –ª–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç —Å –≤–∞—à–∏–º –Ω–æ–≤—ã–º –ø–∞—Ä–æ–ª–µ–º.</p>\n          <router-link to=\"/\" class=\"btn btn-primary\">–í–æ–π—Ç–∏ –≤ –ª–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç</router-link>\n        </div>\n        \n        <!-- Password form -->\n        <form v-else-if=\"tokenVerified\" @submit.prevent=\"resetPassword\" class=\"reset-form\">\n          <h3>{{ isFirstTimeSetup ? '–°–æ–∑–¥–∞–π—Ç–µ –ø–∞—Ä–æ–ª—å –¥–ª—è –≤—Ö–æ–¥–∞' : '–£—Å—Ç–∞–Ω–æ–≤–∫–∞ –Ω–æ–≤–æ–≥–æ –ø–∞—Ä–æ–ª—è' }}</h3>\n          \n          <div v-if=\"partnerInfo\" class=\"partner-info\">\n            <div class=\"info-icon\">üë§</div>\n            <p class=\"info-text\">{{ partnerInfo.email }}</p>\n            <p v-if=\"isFirstTimeSetup\" class=\"info-hint\">–ü—Ä–∏–¥—É–º–∞–π—Ç–µ –Ω–∞–¥—ë–∂–Ω—ã–π –ø–∞—Ä–æ–ª—å –¥–ª—è –¥–æ—Å—Ç—É–ø–∞ –≤ –ª–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç</p>\n          </div>\n          \n          <div class=\"form-group\">\n            <label for=\"password\">–ù–æ–≤—ã–π –ø–∞—Ä–æ–ª—å</label>\n            <div class=\"password-input\">\n              <input\n                id=\"password\"\n                v-model=\"formData.password\"\n                :type=\"showPassword ? 'text' : 'password'\"\n                required\n                minlength=\"6\"\n                placeholder=\"–í–≤–µ–¥–∏—Ç–µ –Ω–æ–≤—ã–π –ø–∞—Ä–æ–ª—å (–º–∏–Ω–∏–º—É–º 6 —Å–∏–º–≤–æ–ª–æ–≤)\"\n                :class=\"{ error: errors.password }\"\n                autocomplete=\"new-password\"\n              />\n              <button \n                type=\"button\" \n                @click=\"showPassword = !showPassword\"\n                class=\"password-toggle\"\n                aria-label=\"–ü–æ–∫–∞–∑–∞—Ç—å –ø–∞—Ä–æ–ª—å\"\n              >\n                {{ showPassword ? 'üôà' : 'üëÅÔ∏è' }}\n              </button>\n            </div>\n            <div v-if=\"errors.password\" class=\"error-message\">{{ errors.password }}</div>\n            <div v-if=\"formData.password\" class=\"password-strength\">\n              <div class=\"strength-bar\">\n                <div \n                  :class=\"['strength-fill', passwordStrength.class]\"\n                  :style=\"{ width: passwordStrength.width }\"\n                ></div>\n              </div>\n              <div class=\"strength-text\">–ù–∞–¥—ë–∂–Ω–æ—Å—Ç—å: {{ passwordStrength.text }}</div>\n            </div>\n            <div v-else class=\"password-hint\">\n              üí° –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –º–∏–Ω–∏–º—É–º 6 —Å–∏–º–≤–æ–ª–æ–≤. –î–ª—è –Ω–∞–¥—ë–∂–Ω–æ—Å—Ç–∏ –¥–æ–±–∞–≤—å—Ç–µ –±—É–∫–≤—ã, —Ü–∏—Ñ—Ä—ã –∏ —Å–ø–µ—Ü—Å–∏–º–≤–æ–ª—ã\n            </div>\n          </div>\n          \n          <div class=\"form-group\">\n            <label for=\"confirmPassword\">–ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ –ø–∞—Ä–æ–ª—è</label>\n            <div class=\"password-input\">\n              <input\n                id=\"confirmPassword\"\n                v-model=\"formData.confirmPassword\"\n                :type=\"showConfirmPassword ? 'text' : 'password'\"\n                required\n                placeholder=\"–ü–æ–≤—Ç–æ—Ä–∏—Ç–µ –Ω–æ–≤—ã–π –ø–∞—Ä–æ–ª—å\"\n                :class=\"{ error: errors.confirmPassword }\"\n                autocomplete=\"new-password\"\n              />\n              <button \n                type=\"button\" \n                @click=\"showConfirmPassword = !showConfirmPassword\"\n                class=\"password-toggle\"\n                aria-label=\"–ü–æ–∫–∞–∑–∞—Ç—å –ø–∞—Ä–æ–ª—å\"\n              >\n                {{ showConfirmPassword ? 'üôà' : 'üëÅÔ∏è' }}\n              </button>\n            </div>\n            <div v-if=\"errors.confirmPassword\" class=\"error-message\">{{ errors.confirmPassword }}</div>\n            <div v-else-if=\"formData.confirmPassword && formData.password === formData.confirmPassword\" class=\"success-message\">\n              ‚úì –ü–∞—Ä–æ–ª–∏ —Å–æ–≤–ø–∞–¥–∞—é—Ç\n            </div>\n          </div>\n          \n          <div class=\"form-actions\">\n            <button \n              type=\"submit\" \n              :disabled=\"loading || !isFormValid\"\n              class=\"btn btn-primary btn-full\"\n            >\n              <span v-if=\"loading\" class=\"btn-loader\">‚è≥</span>\n              {{ loading ? '–£—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø–∞—Ä–æ–ª—è...' : (isFirstTimeSetup ? '–°–æ–∑–¥–∞—Ç—å –ø–∞—Ä–æ–ª—å –∏ –≤–æ–π—Ç–∏' : '–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –ø–∞—Ä–æ–ª—å') }}\n            </button>\n          </div>\n          \n          <div class=\"help-links\">\n            <router-link to=\"/\">‚Üê –í–µ—Ä–Ω—É—Ç—å—Å—è –∫ –≤—Ö–æ–¥—É</router-link>\n          </div>\n        </form>\n      </div>\n    </div>\n    \n    <!-- Notifications -->\n    <AppNotifications />\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted, watch } from 'vue'\nimport { useRoute, useRouter } from 'vue-router'\nimport AppLoader from '@/components/AppLoader.vue'\nimport AppNotifications from '@/components/AppNotifications.vue'\nimport { useNotificationStore } from '@/stores/notifications'\nimport api from '@/plugins/axios'\n\nconst route = useRoute()\nconst router = useRouter()\nconst notificationStore = useNotificationStore()\n\n// Reactive data\nconst loading = ref(false)\nconst tokenVerified = ref(false)\nconst tokenError = ref('')\nconst passwordResetSuccess = ref(false)\nconst partnerInfo = ref(null)\nconst isFirstTimeSetup = ref(true) // –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é —Å—á–∏—Ç–∞–µ–º —á—Ç–æ —ç—Ç–æ –ø–µ—Ä–≤–∞—è —É—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø–∞—Ä–æ–ª—è\n\nconst formData = ref({\n  password: '',\n  confirmPassword: ''\n})\n\nconst errors = ref({})\nconst showPassword = ref(false)\nconst showConfirmPassword = ref(false)\n\n// Computed properties\nconst passwordStrength = computed(() => {\n  const password = formData.value.password\n  if (!password) return { width: '0%', class: '', text: '' }\n  \n  let score = 0\n  let feedback = []\n  \n  // Length check\n  if (password.length >= 6) score += 1\n  if (password.length >= 8) score += 1\n  if (password.length >= 12) score += 1\n  \n  // Character variety checks\n  if (/[a-z]/.test(password)) score += 1\n  if (/[A-Z]/.test(password)) score += 1\n  if (/[0-9]/.test(password)) score += 1\n  if (/[^A-Za-z0-9]/.test(password)) score += 1\n  \n  // Determine strength\n  let strength = ''\n  let width = '0%'\n  let className = ''\n  \n  if (score <= 2) {\n    strength = '–°–ª–∞–±—ã–π'\n    width = '25%'\n    className = 'weak'\n  } else if (score <= 4) {\n    strength = '–°—Ä–µ–¥–Ω–∏–π'\n    width = '50%'\n    className = 'medium'\n  } else if (score <= 6) {\n    strength = '–•–æ—Ä–æ—à–∏–π'\n    width = '75%'\n    className = 'good'\n  } else {\n    strength = '–û—Ç–ª–∏—á–Ω—ã–π'\n    width = '100%'\n    className = 'excellent'\n  }\n  \n  return { width, class: className, text: strength }\n})\n\nconst isFormValid = computed(() => {\n  return formData.value.password.length >= 6 && \n         formData.value.password === formData.value.confirmPassword &&\n         !Object.keys(errors.value).length\n})\n\n// Methods\nconst verifyToken = async () => {\n  const token = route.query.token\n  \n  if (!token) {\n    tokenError.value = '–¢–æ–∫–µ–Ω –Ω–µ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω'\n    return\n  }\n  \n  try {\n    loading.value = true\n    const response = await api.get(`/auth/reset-password/${token}`)\n    partnerInfo.value = {\n      partnerId: response.data.partnerId,\n      email: response.data.partnerEmail\n    }\n    tokenVerified.value = true\n  } catch (error) {\n    console.error('–û—à–∏–±–∫–∞ –ø—Ä–æ–≤–µ—Ä–∫–∏ —Ç–æ–∫–µ–Ω–∞:', error)\n    tokenError.value = error.response?.data?.message || '–ù–µ–¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–π –∏–ª–∏ –∏—Å—Ç–µ–∫—à–∏–π —Ç–æ–∫–µ–Ω'\n  } finally {\n    loading.